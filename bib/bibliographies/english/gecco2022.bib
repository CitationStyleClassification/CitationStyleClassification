  publisher =	{Association for Computing Machinery},
  publisher_address =	{New York, NY, USA},
  note =	{},
  keywords =	{Neuroevolution, reinforcement learning, exploration, quality diversity, evolutionary algorithm},
  ISBN13 =	{978-1-4503-9237-2},
