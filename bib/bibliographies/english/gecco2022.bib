  publisher =	{Association for Computing Machinery},
  publisher_address =	{New York, NY, USA},
  note =	{},
  keywords =	{Evolutionary Combinatorial Optimization and Metaheuristics, variable interaction graph, gray-box optimization, iterated local search},
  ISBN13 =	{978-1-4503-9237-2},
