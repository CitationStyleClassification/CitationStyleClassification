ACM 1-59593-097-3/05/0006},
)

@InProceedings(Llora:gecco05ws,
  author = {Xavier Llor{\`a} and Kumara Sastry and David E. Goldberg},
  title = {Binary Rule Encoding Schemes:  A Study Using The Compact Classifier System},
