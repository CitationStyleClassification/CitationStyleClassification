 organisation = {SIGEVO},
 address = {Dublin, Ireland},
 doi = {doi:10.1145/2001576.2001729},
 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {We propose a cooperative coevolutionary genetic algorithm for learning Bayesian network structures from fully observable data sets. Since this problem can be decomposed into two dependent subproblems, that is to find an ordering of the nodes and an optimal connectivity matrix, our algorithm uses two subpopulations, each one representing a subtask. We describe the empirical results obtained with simulations of the Alarm and Insurance networks. We show that our algorithm outperforms the deterministic algorithm K2.},
 notes = {Also known as \cite{2001729}
GECCO-2011
        A joint meeting of the twentieth international conference on
        genetic algorithms (ICGA-2011) and the sixteenth annual genetic
        programming conference (GP-2011)},
