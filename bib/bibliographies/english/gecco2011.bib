 keywords = {Evolutionary multiobjective optimization},
 month = {12-16 July},
 organisation = {SIGEVO},
 address = {Dublin, Ireland},
 doi = {doi:10.1145/2001576.2001685},
 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {Several comparison-based complexity results have been published recently, including multi-objective optimisation. However, these results are, in the multiobjective case, quite pessimistic, due to the huge family of fitness functions considered. Combining assumptions on fitness functions and traditional comparison-based assumptions, we get more realistic bounds emphasising the importance of reducing the number of conflicting objectives for reducing the runtime of multiobjective optimisation. The approach can in particular predict lower bounds on the computation time, depending on the type of requested convergence: pointwise, or to the whole Pareto set. Also, a new (untested yet) algorithm is proposed for approximating the whole Pareto set.},
 notes = {Also known as \cite{2001685}
GECCO-2011
        A joint meeting of the twentieth international conference on
        genetic algorithms (ICGA-2011) and the sixteenth annual genetic
        programming conference (GP-2011)},
 }

@inproceedings{Blesken:2011:GECCO,
 author = {Matthias W. Blesken and Anouar Chebil and Ulrich Rueckert and Xavier Esquivel and Oliver Schuetze},
 title = {Integrated circuit optimization by means of evolutionary multi-objective optimization},
