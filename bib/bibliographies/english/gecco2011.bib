 organisation = {SIGEVO},
 address = {Dublin, Ireland},
 doi = {doi:10.1145/2001576.2001712},
 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {Linkage in terms of genetic algorithms is a measure of interdependence of groups of genes. When linkage exists, the fitness contribution of one gene depends on the allele setting of another. Our approach, a surrogate-assisted linkage inference genetic algorithm (SALIGA), is able to detect linkage for real-valued alleles. It uses a perturbation-based method with the aid of fitness surrogates and clustering techniques. Experimental results of linkage inference applied to several synthetic fitness functions are provided. The results demonstrate that SALIGA is able to reliably infer linkage and will correctly group the genes of an individual according to their linkage group membership, while using fewer fitness evaluations through a surrogate model. In addition, our results are augmented with a discussion regarding linkage detection through feature selection.},
 notes = {Also known as \cite{2001712}
GECCO-2011
        A joint meeting of the twentieth international conference on
        genetic algorithms (ICGA-2011) and the sixteenth annual genetic
        programming conference (GP-2011)},
 }

@inproceedings{Pelikan:2011:GECCOp,
 author = {Martin Pelikan and Mark W. Hauschild and Dirk Thierens},
 title = {Pairwise and problem-specific distance metrics in the linkage tree genetic algorithm},
 booktitle = {GECCO '11: Proceedings of the 13th annual conference on Genetic and evolutionary computation},
