@inproceedings{PetricaPop:2011:GECCO,
 author = {Petrica C. Pop and Serban Iordache},
 title = {A hybrid heuristic approach for solving the generalized traveling salesman problem},
 booktitle = {GECCO '11: Proceedings of the 13th annual conference on Genetic and evolutionary computation},
