@inproceedings{Toledo:2011:GECCO,
 author = {Claudio Fabiano Motta Toledo and M\'{a}rcio da Silva Arantes and Paulo Morelato Fran\c{c}a},
 title = {Tabu search to solve the synchronized and integrated two-level lot sizing and scheduling problem},
