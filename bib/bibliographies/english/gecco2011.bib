 address = {Dublin, Ireland},
 doi = {doi:10.1145/2001576.2001686},
 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {The design of resource efficient integrated circuits (ICs) requires solving a minimisation problem which consists of more than one objective given as measures of the available resources. This multi-objective optimisation problem (MOP) can be solved on the smallest unit of the IC, the standard cells, to improve the performance of the entire circuit. In this work, transistor sizing of an IC is approached via a multi-objective approach which includes the use of multi-objective evolutionary algorithms (MOEAs). We compare the performance of two MOEAs on a four-dimensional MOP of a particular standard cell. The results indicate that evolutionary strategies are suitable for the treatment of such problems and advantageous against other rather classical methods.},
 notes = {Also known as \cite{2001686}
GECCO-2011
        A joint meeting of the twentieth international conference on
        genetic algorithms (ICGA-2011) and the sixteenth annual genetic
        programming conference (GP-2011)},
 }

@inproceedings{Finck:2011:GECCO,
