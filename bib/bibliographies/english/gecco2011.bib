 editor = {Natalio Krasnogor and Pier Luca Lanzi and Andries Engelbrecht and David Pelta and Carlos Gershenson and Giovanni Squillero and Alex Freitas and Marylyn Ritchie and Mike Preuss and Christian Gagne and Yew Soon Ong and Guenther Raidl and Marcus Gallager and Jose Lozano and Carlos Coello-Coello and Dario Landa Silva and Nikolaus Hansen and Silja Meyer-Nieberg and Jim Smith and Gus Eiben and Ester Bernado-Mansilla and Will Browne and Lee Spector and Tina Yu and Jeff Clune and Greg Hornby and Man-Leung Wong and Pierre Collet and Steve Gustafson and Jean-Paul Watson and Moshe Sipper and Simon Poulding and Gabriela Ochoa and Marc Schoenauer and Carsten Witt and Anne Auger},
 isbn13 = {978-1-4503-0557-0},
 pages = {1891--1898},
 keywords = {Search-based software engineering},
 month = {12-16 July},
 organisation = {SIGEVO},
 address = {Dublin, Ireland},
 doi = {doi:10.1145/2001576.2001830},
 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {The aim of our research is to find an efficient solution to the services QoS optimization problem. This NP-hard problem is well known in the service-oriented computing field: given a business workflow that includes a set of abstract services and a set of concrete service implementations for each abstract service, the goal is to find the optimal combination of concrete services. The majority of recent proposals indicate the Genetic Algorithms (GA) as the best approach for complex workflows. But this problem usually needs to be solved at runtime, a task for which GA may be too slow. We propose a new approach, based on Differential Evolution (DE), that converges faster and it is more scalable and robust than the existing solutions based on Genetic Algorithms.},
 notes = {Also known as \cite{2001830}
GECCO-2011
        A joint meeting of the twentieth international conference on
