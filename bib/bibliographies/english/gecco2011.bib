@inproceedings{Hauschild:2011:GECCO,
 author = {Mark Hauschild and Martin Pelikan},
 title = {Advanced neighborhoods and problem difficulty measures},
 booktitle = {GECCO '11: Proceedings of the 13th annual conference on Genetic and evolutionary computation},
