@inproceedings{deFreitas:2011:GECCO,
 author = {Alan R.R. {de Freitas} and Frederico Gadelha Guimaraes},
 title = {Originality and diversity in the artificial evolution of melodies},
 booktitle = {GECCO '11: Proceedings of the 13th annual conference on Genetic and evolutionary computation},
 year = {2011},
