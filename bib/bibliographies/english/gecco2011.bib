 }

@inproceedings{Lim:2011:GECCO,
 author = {Soo Ling Lim and Peter J. Bentley},
 title = {Evolving relationships between social networks and stakeholder involvement in software projects},
 booktitle = {GECCO '11: Proceedings of the 13th annual conference on Genetic and evolutionary computation},
