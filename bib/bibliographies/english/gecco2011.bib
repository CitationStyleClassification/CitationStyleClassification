@inproceedings{Pelikan:2011:GECCO,
 author = {Martin Pelikan},
 title = {Analysis of epistasis correlation on NK landscapes with nearest-neighbor interactions},
 booktitle = {GECCO '11: Proceedings of the 13th annual conference on Genetic and evolutionary computation},
