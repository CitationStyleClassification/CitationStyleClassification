@inproceedings{Kommenda:2012:GECCOcomp,
 author = {Michael Kommenda and Gabriel Kronberger and Stefan Wagner and Stephan Winkler and Michael Affenzeller},
 title = {On the architecture and implementation of tree-based genetic programming in HeuristicLab},
 booktitle = {GECCO 2012 Evolutionary Computation Software Systems (EvoSoft)},
 year = {2012},
