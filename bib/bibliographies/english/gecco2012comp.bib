 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {DEAP (Distributed Evolutionary Algorithms in Python) is a novel volutionary computation framework for rapid prototyping and testing of ideas. Its design departs from most other existing frameworks in that it seeks to make algorithms explicit and data structures transparent, as opposed to the more common black box type of frameworks. It also incorporates easy parallelism where users need not concern themselves with gory implementation details like synchronization and load balancing, only functional decomposition. Several examples illustrate the multiple properties of DEAP.},
 notes = {Also known as \cite{2330799}
Distributed at GECCO-2012.

ACM Order Number 910122.},

 }

