@inproceedings{Longo:2012:GECCOcomp,
 author = {Giuseppe Longo and Ma\"{e}l Mont\'{e}vil and Stuart Kauffman},
 title = {No entailing laws, but enablement in the evolution of the biosphere},
 booktitle = {GECCO 2012 Keynote talks},
 year = {2012},
 editor = {Jason H. Moore},
 isbn13 = {978-1-4503-1178-6},
 keywords = {},
 pages = {1379--1392},
