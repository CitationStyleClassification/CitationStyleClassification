publisher_address = {New York, NY, USA},
 abstract = {We study a matrix representation for an EA attack on the CCPOP with transaction costs. The representation is based on portfolio sequences which change over the investment lifetime in response to asset price changes. We show the approach is effective and that EA performance is directly related to asset price correlation. We compare the EA with a matrix hill climber and show some common results of vector representations do not hold for a matrix one, potentially providing a step forward in performance of such algorithms.},
