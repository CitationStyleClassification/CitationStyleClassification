 doi = {doi:10.1145/2464576.2464622},
 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {Decomposition based approaches are known to perform well on many-objective problems when a suitable set of weights is provided. However, providing a suitable set of weights a priori is difficult. This study proposes a novel algorithm: preference-inspired co-evolutionary algorithm using weights (PICEA-w), which co-evolves a set of weights with the usual population of candidate solutions during the search process. The co-evolution enables suitable sets of weights to be constructed along the optimisation process, thus guiding the candidate solutions toward the Pareto optimal front. Experimental results show PICEA-w performs better than algorithms embedded with random or uniform weights.},
