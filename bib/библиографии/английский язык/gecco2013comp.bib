 month = {6-10 July},
 organisation = {SIGEVO},
 address = {Amsterdam, The Netherlands},
 doi = {doi:10.1145/2464576.2464620},
 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {In on-line bin-packing problems, a policy must be found for assigning items, according their size, immediately upon their arrival to bins with known initial capacities. In previous work of Ozcan and Parkes (GECCO 2011), a policy was represented as a 2-dimensional matrix (array) and good matrices were then evolved using a genetic algorithm (GA). Here, we consider a form of dimensional reduction in which variables in the matrix are grouped into elements taken from one-dimensional vectors. We find that with the right form of grouping, the GA then typically finds such vector policies significantly more quickly, and yet suffers little loss of overall quality.},
 notes = {Also known as \cite{2464620}
Distributed at GECCO-2013.},
 }
