 organisation = {SIGEVO},
 address = {Amsterdam, The Netherlands},
 doi = {doi:10.1145/2464576.2464649},
 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {Following a recent call for a suite of benchmarks for genetic programming, we investigate the criteria for a meaningful dynamic benchmark for GP. We explore the design of a dynamic benchmark for symbolic regression, based on semantic distance between evaluated functions, where larger distances serve as a proxy for greater environmental change. We do not find convincing evidence that lower semantic distance is a good proxy for greater ease in adapting to a change. We conclude that due to fundamental characteristics of GP, it is difficult to come up with a single dynamic benchmark problem which is generally applicable.},
