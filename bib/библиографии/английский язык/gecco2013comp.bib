 publisher = {ACM},
publisher_address = {New York, NY, USA},
 abstract = {In this paper a novel constructive hyperheuristic for CVRP is proposed. This hyperheuristic, called HyperPOEMS, is based on an evolutionary-based iterative local search algorithm. Its inherent characteristics make it capable of autonomously searching a structured space of low-level domain specific heuristics for their suitable combinations that produce good solutions to particular problem instance. HyperPOEMS was tested on standard benchmarks and compared to two existing constructive hyperheuristic, HHC-VRP and EHH-VRP. The results show that HyperPOEMS outperforms both compared hyperheuristics and produces solutions competitive to solutions obtained by specialised metaheuristics designed for CVRP.},
 notes = {Also known as \cite{2464684}
