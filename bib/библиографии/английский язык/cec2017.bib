@INPROCEEDINGS{gutierrez-rodríguez:2017:CEC,
author={A. E. Gutierrez-Rodríguez and J. C. Ortiz-Bayliss and A.
Rosales-Pérez and I. M. Amaya-Contreras and S. E. Conant-Pablos and H.
Terashima-Marín and C. A. C. Coello},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
