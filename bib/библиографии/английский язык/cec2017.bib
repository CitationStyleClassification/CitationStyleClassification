@INPROCEEDINGS{silveira:2017:CEC,
author={L. Â. da Silveira and J. L. Soncco-Álvarez and M. Ayala-Rincón},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={Parallel genetic algorithms with sharing of individuals for
