@INPROCEEDINGS{banda:2017:CEC,
author={J. Banda and J. Velasco and A. Berrones},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={A hybrid heuristic algorithm based on Mean-Field Theory with a
