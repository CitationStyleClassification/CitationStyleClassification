  notes 	 = {IEEE Catalog Number: CFP17ICE-ART
        	    Also known as \cite{7969424}},
}

@INPROCEEDINGS{latorre:2017:CEC,
author={A. LaTorre and J. M. Peña},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={A comparison of three large-scale global optimizers on the CEC
