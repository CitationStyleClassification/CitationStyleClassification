@INPROCEEDINGS{vijay:2017:CEC,
author={R. Vijay and S. J. Nanda},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={Declustering of an earthquake catalog based on ergodicity using
parallel Grey Wolf Optimization},
year={2017},
  editor	= {Jose A. Lozano},
