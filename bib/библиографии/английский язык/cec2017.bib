}

@INPROCEEDINGS{li:2017:CEC,
author={Y. Li and T. Yue and S. Ali and L. Zhang},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={A multi-objective and cost-aware optimization of requirements
assignment for review},
