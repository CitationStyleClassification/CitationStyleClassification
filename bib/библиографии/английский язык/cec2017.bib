@INPROCEEDINGS{musilek:2017:CEC,
author={P. Musilek and P. Krömer and R. Martins and H. C. Hesse},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={Optimal energy management of residential PV/HESS using
