@INPROCEEDINGS{ghosh:2017:CEC,
author={A. Ghosh and S. Das and B. K. Panigrahi and A. K. Das},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={A noise resilient Differential Evolution with improved parameter
and strategy control},
year={2017},
