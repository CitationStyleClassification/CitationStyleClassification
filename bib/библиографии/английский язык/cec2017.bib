@INPROCEEDINGS{ribaric:2017:CEC,
author={T. Ribaric and S. Houghten},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={Genetic programming for improved cryptanalysis of elliptic curve
