@INPROCEEDINGS{yu:2017:CEC,
author={Haibo Yu and Ying Tan and Chaoli Sun and Jianchao Zeng},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={Clustering-based evolution control for surrogate-assisted
particle swarm optimization},
year={2017},
  editor	= {Jose A. Lozano},
  pages 	= {503--508},
  address	= {Donostia, San Sebastian, Spain},
  publisher	= {IEEE},
  ISBN13	= {978-1-5090-4601-0},
abstract={When using a fixed number of neighbors for training a local
surrogate model in surrogate assisted evolutionary optimization
algorithms, it may suffer from the large uncertainty because the actual
distribution of candidate's neighborhood may be neglected. In this
paper, we propose to firstly analyze the distribution characteristics of
candidate's neighborhood through a modified overlapping clustering
method before training a local surrogate, and then use the clustering
