}

@INPROCEEDINGS{simões:2017:CEC,
author={C. Simões and R. Neves and N. Horta},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={Using sentiment from Twitter optimized by Genetic Algorithms to
predict the stock market},
