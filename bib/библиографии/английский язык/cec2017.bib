@INPROCEEDINGS{fang:2017:CEC,
author={W. Fang and L. Zhang and J. Zhou and X. Wu and J. Sun},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={A novel quantum-behaved particle swarm optimization with random
