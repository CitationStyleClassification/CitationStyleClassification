}

@INPROCEEDINGS{kabadurmus:2017:CEC,
author={O. Kabadurmus and M. S. Erdogan and M. F. Tasgetiren},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={Design of multi-product multi-period two-echelon supply chain
