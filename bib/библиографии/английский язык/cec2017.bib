@INPROCEEDINGS{tyasnurita:2017:CEC,
author={R. Tyasnurita and E. Özcan and R. John},
booktitle={2017 IEEE Congress on Evolutionary Computation (CEC)},
title={Learning heuristic selection using a Time Delay Neural Network
