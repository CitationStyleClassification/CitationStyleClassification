  title = "Rigorous Time Complexity Analysis of Univariate Marginal >Distribution Algorithm with Margins",
  booktitle = "2009 IEEE Congress on Evolutionary Computation",
  year = 2009,
  editor = "Andy Tyrrell",
  pages = {},
