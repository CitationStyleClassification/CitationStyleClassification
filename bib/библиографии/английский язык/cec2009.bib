  title = "A Parallel Genetic Algorithm for Protein Folding Prediction Using the 3D-HP Side Chain Model",
  booktitle = "2009 IEEE Congress on Evolutionary Computation",
  year = 2009,
  editor = "Andy Tyrrell",
  pages = {},
  address = "Trondheim, Norway",
